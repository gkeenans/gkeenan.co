{% for year, avgab in collections.avgab | groupby("data.year") | dictsort | reverse %}

<ul class="audiobloglist">
  {% for post in avgab | reverse %}
  {% set tags = post.data.tags %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a><br />
    <p class="postlist-meta"><time>{{ post.data.date | postDate }}</time></p>
  </li>
  {% endfor %}
</ul>
{% endfor %}